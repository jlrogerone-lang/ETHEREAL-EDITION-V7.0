{
  "expo": {
    "name": "ETHEREAL",
    "slug": "ethereal-v7",
    "version": "7.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "icon": "./assets/icon.png",
    "splash": {
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ethereal.luxe",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "Para entrada de voz en Le Nez AI"
      }
    },
    "android": {
      "package": "com.ethereal.luxe",
      "versionCode": 1,
      "adaptiveIcon": {
        "backgroundColor": "#000000"
      },
      "permissions": [
        "VIBRATE",
        "RECORD_AUDIO"
      ]
    },
    "plugins": [
      "expo-av"
    ],
    "extra": {
      "eas": {
        "projectId": "ethereal-v7"
      }
    }
  }
}
