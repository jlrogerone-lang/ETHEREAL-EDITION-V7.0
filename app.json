{
  "expo": {
    "name": "L'Essence du Luxe",
    "slug": "ethereal-omni",
    "version": "8.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "icon": "./assets/icon.png",
    "splash": {
      "backgroundColor": "#050505"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.essenceduluxe.app",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "Para entrada de voz en Le Nez AI",
        "NSCameraUsageDescription": "Para Le Sommelier Visuel y escáner OCR",
        "NSPhotoLibraryUsageDescription": "Para seleccionar fotos de outfit",
        "NSFaceIDUsageDescription": "Autenticación biométrica segura"
      }
    },
    "android": {
      "package": "com.essenceduluxe.app",
      "versionCode": 8,
      "adaptiveIcon": {
        "backgroundColor": "#050505"
      },
      "permissions": [
        "VIBRATE",
        "RECORD_AUDIO",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "USE_BIOMETRIC",
        "USE_FINGERPRINT",
        "RECEIVE_BOOT_COMPLETED"
      ]
    },
    "plugins": [
      "expo-av",
      "expo-camera",
      "expo-image-picker",
      "expo-notifications",
      "expo-local-authentication"
    ],
    "extra": {
      "eas": {
        "projectId": "ethereal-omni"
      }
    }
  }
}
